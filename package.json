{
  "name": "baseball-stats-frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.8.3",
    "react-autosuggest": "^9.4.3",
    "react-dom": "^16.8.3",
    "react-router-dom": "^5.1.2",
    "react-scripts": "2.1.5",
    "autoprefixer": "^9.6.5",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^6.1.3",
    "tailwindcss": "^1.1.2"
  },
  "scripts": {
    "start": "npm-run-all --parallel watch:css start:react",
    "build": "npm-run-all build:css build:react",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "watch:css": "postcss src/tailwind.css -o src/index.css -w",
    "start:react": "react-scripts --max_old_space_size=4096 start",
    "build:react": "react-scripts --max_old_space_size=4096 build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
  }
}
